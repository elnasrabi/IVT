{
  "name": "virtualized",
  "version": "0.1.0",
  "description": "Virtualized list and table components for React",
  "main": "source/index.js",
  "scripts": {
    "build": "webpack --config webpack.build.js -c --bail",
    "clean": "rm -rf dist/*",
    "dev": "webpack-dev-server --hot --progress --port 8081 & webpack --config webpack.build.js --watch",
    "link": "npm run link:source && npm run link:vendor",
    "link:source": "link-package ./source @source",
    "link:vendor": "link-package ./vendor @vendor",
    "tdd": "npm run test:unit -- --auto-watch --no-single-run",
    "test:unit": "NODE_ENV=test karma start",
    "test": "npm run lint && npm run test:unit",
    "lint": "standard"
  },
  "author": "Brian Vaughn",
  "license": "MIT",
  "engines": {
    "node": "5.0.0"
  },
  "babel": {
    "stage": 0
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "dist",
      "vendor"
    ],
    "global": [
      "after",
      "afterEach",
      "before",
      "beforeEach",
      "cancelAnimationFrame",
      "describe",
      "it",
      "requestAnimationFrame"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "standard",
      "standard-react"
    ],
    "globals": {
      "describe": false,
      "it": false
    }
  },
  "dependencies": {
    "babel": "^5.8.29",
    "babel-core": "^5.8.33",
    "classnames": "^2.1.5",
    "dom-helpers": "^2.4.0",
    "react": "^0.14.1",
    "react-addons-css-transition-group": "^0.14.1",
    "react-dom": "^0.14.1",
    "react-pure-render": "^1.0.2",
    "webpack": "^1.12.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.3",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-plugin-typecheck": "^2.0.0",
    "bluebird": "^3.0.2",
    "css-loader": "^0.21.0",
    "eslint": "^1.7.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-config-standard-react": "^1.1.0",
    "eslint-plugin-react": "^3.6.3",
    "eslint-plugin-standard": "^1.3.1",
    "expect.js": "^0.3.1",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "json-loader": "^0.5.3",
    "karma": "^0.13.14",
    "karma-junit-reporter": "^0.3.8",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-spec-reporter": "0.0.22",
    "karma-webpack": "^1.7.0",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "link-package": "^0.1.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.3.3",
    "null-loader": "^0.1.1",
    "phantomjs": "^1.9.18",
    "postcss-loader": "^0.7.0",
    "raf": "^3.1.0",
    "react-addons-test-utils": "^0.14.1",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "standard": "^5.3.1",
    "stats-webpack-plugin": "^0.2.2",
    "style-loader": "^0.13.0",
    "webpack-dev-server": "^1.12.0"
  }
}
